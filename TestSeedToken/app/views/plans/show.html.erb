<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @plan.name %>
</p>

<p>
  <% for semester in @plan.semesters %>
    <b>Semester: <%= semester.term %> </b>
    <ul>
      <% for course in semester.courses %>
        <li><%= course.name %> </li>
      <% end %>
    </ul>
   <% end %> 
</p>

<%= link_to 'Edit', edit_plan_path(@plan) %> |
<%= link_to 'Back', plans_path %>
